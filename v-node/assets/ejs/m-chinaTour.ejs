@@include("public/m-header.ejs", {
"plugin": [],
"private": ["../css/m-chinaTour.css"]
})
<div class="clearfix">
    <div class="china-tour">
        <div class="slogan"><img src="../img/chinaTour/slogan.png" alt="火星训练营邀请你报名"></div>
        <div class="site" id="city" data-id="<%= urlId %>"></div>
        <div class="china-tour-from">
            <ul>
                <li><p><span>Name</span><font>姓名</font></p><div class="text-box"><input type="text" id="name"></div></li>
                <li><p><span>Company</span><font>公司</font></p><div class="text-box"><input type="text" id="Firm"></div></li>
                <li><p><span>Phone NO</span><font>电话</font></p><div class="text-box"><input type="text" id="Phone"></div></li>
                <li><p><span>E-mail</span><font>邮箱</font></p><div class="text-box"><input type="text" id="EMail"></div></li>
                <li><p class="grow"><span>Channel</span><font>了解的渠道</font></p></li>
            </ul>
            <div class="source">
                <div class="item">
                    <input name="intro104" id="radio1" value="4" type="radio">
                    <label for="radio1"></label><p>微信</p>
                </div>
                <div class="item">
                    <input name="intro104" id="radio2" value="1" type="radio">
                    <label for="radio2"></label><p>朋友推荐</p>
                </div>
                <div class="item">
                    <input name="intro104" id="radio3" value="2" type="radio">
                    <label for="radio3"></label><p>火星社群</p>
                </div>
                <div class="item">
                    <input name="intro104" id="radio4" value="5" type="radio">
                    <label for="radio4"></label><p>其他</p>
                </div>
            </div>
            <div class="position clearfix">
                <p><span>Position </span><font>职业属性</font></p>
            </div>
            <div class="identity position-h">
                <div class="item">
                    <input name="intro105" id="position1" value="创始人" type="radio">
                    <label for="position1"></label><p>创始人</p>
                </div>
                <div class="item">
                    <input name="intro105" id="position2" value="创业者" type="radio">
                    <label for="position2"></label><p>创业者</p>
                </div>
                <div class="item">
                    <input name="intro105" id="position3" value="投资机构" type="radio">
                    <label for="position3"></label><p>投资机构</p>
                </div>
                <div class="item">
                    <input name="intro105" id="position4" value="区块链从业者" type="radio">
                    <label for="position4"></label><p>区块链从业者</p>
                </div>
                <div class="item">
                    <input name="intro105" id="position5" value="其他" type="radio">
                    <label for="position5"></label><p>其他</p>
                </div>
            </div>
            <div class="activity time">
                <h6>活动时间</h6>
                <div class="activity-time">
                    <p></p>
                </div>
            </div>
            <div class="activity site-b">
                <h6>活动地点</h6>
                <div class="activity-cont">
                    <p></p>
                </div>
            </div>
            <div class="apply-btn" id="applyBtn">提交</div>
        </div>
    </div>
    <div class="videoplay-mask" id="videoplayMask"></div>
    <div class="succeed-mtc">
        <div class="succeed-slogan"><img src="../img/m-apply/succeed-title-mtc.png" alt=""></div>
        <div class="succeed-text"><img src="../img/m-apply/succeed-text.png" alt=""></div>
        <div class="succeed-bottom"><img src="../img/m-apply/succeed-b-mtc.png" alt=""></div>
    </div>
</div>
@@include("public/m-footer-prue.ejs", {
"plugin": [],
"private": ["../js/m-chinaTour.js"],
"shreTitle": "<%= webSiteInfo.title %>",
"shareDesc": "<%= webSiteInfo.description %>",
"shareImgurl": "http://static.huoxing24.com/images/wxshare-icon.png"
})
